<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation=" http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AlpineBits Servlet</servlet-name>
        <servlet-class>com.idm.alpinebits.http.impl.AlpineBitsServlet</servlet-class>
        <init-param>
            <param-name>MIDDLEWARE_CLASSNAME</param-name>
            <param-value>com.idm.alpinebits.http.middleware.MultipartFormDataParserMiddleware</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>AlpineBits Servlet</servlet-name>
        <url-pattern>/AlpineBits</url-pattern>
    </servlet-mapping>
</web-app>