      <rng:element name="OTA_HotelDescriptiveContentNotifRS">

        <rng:ref name="def_generic_response"/>

        <rng:attribute name="Version"/>
        <rng:optional>
          <rng:attribute name="TimeStamp"/>
        </rng:optional>
        <rng:optional>
          <rng:attribute name="xsi:schemaLocation"/>
        </rng:optional>

      </rng:element>


