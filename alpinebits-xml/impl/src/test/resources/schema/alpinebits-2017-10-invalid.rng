                              </rng:element>
                            </rng:oneOrMore>
                      
                          </rng:element>
                        </rng:optional>
                      
                        <rng:optional>
                          <rng:element name="DOW_Restrictions">
                      
                            <rng:optional>
                              <rng:element name="ArrivalDaysOfWeek">
                      
                                <rng:optional>
                                  <rng:attribute name="Mon">
                                    <rng:ref name="def_bool"/>
                                  </rng:attribute>
                                </rng:optional>
                                <rng:optional>
                                  <rng:attribute name="Tue">
                                    <rng:ref name="def_bool"/>
                                  </rng:attribute>
                                </rng:optional>
                                <rng:optional>
                                  <rng:attribute name="Weds">
                                    <rng:ref name="def_bool"/>
                                  </rng:attribute>
