                          </rng:choice>
                        </rng:attribute>
                      </rng:optional>

                    </rng:element>
                  </rng:optional>

                  <rng:optional>
                    <rng:attribute name="Start">
                      <rng:ref name="def_date"/>
                    </rng:attribute>
                  </rng:optional>
                  <rng:optional>
                    <rng:attribute name="End">
                      <rng:ref name="def_date"/>
                    </rng:attribute>
