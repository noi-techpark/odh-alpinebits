                          <rng:optional>
                            <rng:attribute name="Tue">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Weds">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Thur">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Fri">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Sat">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Sun">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>

                        </rng:element>
                      </rng:optional>

                      <rng:optional>
                        <rng:element name="DepartureDaysOfWeek">

                          <rng:optional>
                            <rng:attribute name="Mon">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Tue">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Weds">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Thur">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Fri">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
                            <rng:attribute name="Sat">
                              <rng:ref name="def_bool"/>
                            </rng:attribute>
                          </rng:optional>
                          <rng:optional>
